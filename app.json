{
  "expo": {
    "name": "Maraneyma",
    "slug": "Maraneyma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-maraneyma.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
   "splash": {
      "image": "./assets/images/logo-maraneyma.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-maraneyma.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.evelynkga.Maraneyma",
      "softwareKeyboardLayoutMode": "resize"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "hideStatusBar": true
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
